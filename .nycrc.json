{
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "branches": 80,
  "functions": 80,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "dist",
    "src/index.ts",
    "src/**/test/**/*.ts",
    "src/**/tests/**/*.ts"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "all": true
}
